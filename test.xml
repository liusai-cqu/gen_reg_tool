<ipxact:component xmlns:ipxact="http://www.accellera.org/XMLSchema/IPXACT/1685-2014">
  <ipxact:memoryMap>
    <ipxact:name>TEST_RegisterMap</ipxact:name>
  </ipxact:memoryMap>
  <ipxact:addressBlock>
    <ipxact:name>MainBlock</ipxact:name>
    <ipxact:register>
      <ipxact:name>CTRL</ipxact:name>
      <ipxact:addressOffset>0x0</ipxact:addressOffset>
      <ipxact:access>read-write</ipxact:access>
      <ipxact:reset>
        <ipxact:value>0x0</ipxact:value>
      </ipxact:reset>
    </ipxact:register>
    <ipxact:register>
      <ipxact:name>STATUS</ipxact:name>
      <ipxact:addressOffset>0x4</ipxact:addressOffset>
      <ipxact:access>read-only</ipxact:access>
      <ipxact:reset>
        <ipxact:value>0x0</ipxact:value>
      </ipxact:reset>
    </ipxact:register>
    <ipxact:register>
      <ipxact:name>COMMAND</ipxact:name>
      <ipxact:addressOffset>0xA4</ipxact:addressOffset>
      <ipxact:access>write-only</ipxact:access>
      <ipxact:reset>
        <ipxact:value>0x0</ipxact:value>
      </ipxact:reset>
    </ipxact:register>
    <ipxact:register>
      <ipxact:name>CONFIG</ipxact:name>
      <ipxact:addressOffset>0x100</ipxact:addressOffset>
      <ipxact:access>read-write</ipxact:access>
      <ipxact:reset>
        <ipxact:value>0x0</ipxact:value>
      </ipxact:reset>
    </ipxact:register>
    <ipxact:register>
      <ipxact:name>RESERVED</ipxact:name>
      <ipxact:addressOffset>0x108</ipxact:addressOffset>
      <ipxact:access>reserved</ipxact:access>
    </ipxact:register>
  </ipxact:addressBlock>
</ipxact:component>